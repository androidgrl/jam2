<% if current_user %>
  <%= link_to 'Songs', songs_path %><br>
  <%= link_to 'Playlists', playlists_path %><br>
  <% if current_user.admin? %>
    <%= link_to 'Categories', admin_categories_path %>
  <% end %>
<% else %>
  <%= link_to 'Login', login_path %> OR <%= link_to 'Signup', new_user_path %><br>
<% end %>
