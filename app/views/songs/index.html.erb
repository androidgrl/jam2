<%= link_to 'Home', root_path %><br>
<br>
<% @songs.each do |song|  %>
  <%= link_to song.title + ", by " + song.artist, song %> <%= button_to 'Add' %><br>
  <% if current_user.admin? %>
    <%= link_to 'Delete', song, method: :delete %> <%= link_to 'Edit', edit_song_path(song) %><br>
  <% end %>
<% end %>
<%= link_to 'Create new song', new_song_path %>
