<!DOCTYPE html>
<html>
  <head>
    <title>Jam2</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% if current_user %>
      Welcome <%= current_user.username %> <%= link_to 'Logout', logout_path, method: :delete %><br>
    <% end %>
    <%= yield %>
  </body>
</html>
